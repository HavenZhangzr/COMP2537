<%- include("templates/header") %>

<h1 class="mb-3">Hello, <%= username %></h1>
<p class="text-muted mb-4">Welcome to the members area. Enjoy the content!</p>

<div class="row justify-content-center">
  <% images.forEach(img => { %>
    <div class="col-12 col-sm-6 col-md-4 mb-4 d-flex justify-content-center">
      <div class="card shadow-sm" style="width: 100%; max-width: 300px;">
        <img src="/<%= img %>" class="card-img-top rounded" style="height: 300px; object-fit: cover;" alt="Image">
      </div>
    </div>
  <% }) %>
</div>

<%- include("templates/footer") %>
